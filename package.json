{
  "name": "distributed-rate-limiter",
  "version": "0.1.0",
  "description": "A pluggable, Redis-ready rate limiter with fixed-window and token-bucket algorithms for Node/Express.",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server/app.ts",
    "build": "tsc",
    "start": "node dist/server/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "rate-limiter",
    "express",
    "typescript",
    "redis",
    "token-bucket",
    "fixed-window"
  ],
  "author": "Anubhav Majumdar",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
